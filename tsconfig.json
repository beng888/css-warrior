{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitAny": true,
    "baseUrl": ".",
    "paths": {
      // This needs to be mirrored in babel.config.js
      "src/*": ["./src/*"],
      "assets/*": ["./assets/*"]
    }
  }
}
